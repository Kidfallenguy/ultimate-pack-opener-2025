      "package": "com.tuusuario.ultimatepackopener",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "3d05892b-55d4-40d4-996e-96044ec4bf66"
      }
    }
  }
